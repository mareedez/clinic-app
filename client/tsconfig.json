{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json"},
    { "path": "./tsconfig.node.json"}
  ],
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"]
